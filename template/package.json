  "private": true,
  "scripts": {
    "start": "react-native start",
    "pods": "cd ios && pod install && cd ..",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "reset": "find . -type dir -name node_modules -prune -print -exec rm -rf '{}' + && cd ios && rm -rf Pods && cd .",
    "lint": "eslint .",
    "lint:fix": "yarn lint --fix",
    "lint:tsc": "tsc --noEmit",
    "test": "jest",
    "build:e2e": "npx detox build --configuration ios.sim.debug",
    "test:e2e": "npx detox test --configuration ios.sim.debug",
    "test:e2e:watch": "yarn test:e2e --watch",
    "test:watch": "yarn test --watch"
  },
